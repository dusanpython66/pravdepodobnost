          "execution_count": 1
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import itertools\n",
        "#from itertools import powerset\n",
        "narozeniny = [i for i in range(1,366)]\n",
        "#powerset(narozeniny)\n",
        "\n",
        "def factorial(n):\n",
        "    result = 1\n",
        "    for i in range(1, n+1):\n",
        "        result *= i\n",
        "    return result\n",
        "\n",
        "def pravd(k):\n",
        "    p = len(list(itertools.combinations(narozeniny, k))) * factorial(k) / 365**k\n",
        "    return 1 - p"
      ],
